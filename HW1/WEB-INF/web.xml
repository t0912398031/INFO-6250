<web-app>
	<servlet>
        <servlet-name>Part2</servlet-name>
        <servlet-class>HW1_Part2</servlet-class>
    </servlet>
	<servlet>
        <servlet-name>Part4</servlet-name>
        <servlet-class>HW1_Part4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Part5</servlet-name>
        <servlet-class>HW1_Part5</servlet-class>
    </servlet>    
    <servlet>
        <servlet-name>Part6</servlet-name>
        <servlet-class>HW1_Part6</servlet-class>
    </servlet>  
    <servlet>
        <servlet-name>Part7</servlet-name>
        <servlet-class>HW1_Part7</servlet-class>
